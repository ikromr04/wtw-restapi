    "eslint": "^8.45.0",
    "pino-pretty": "^10.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "inversify": "^6.0.1",
    "pino": "^8.15.0",
    "reflect-metadata": "^0.1.13"
  }
}
